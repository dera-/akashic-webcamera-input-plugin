{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/main.js",
	"operationPlugins": [
		{
			"code": 10,
			"script": "./node_modules/@dera-/akashic-webcamera-input-plugin/lib/index.js",
			"option": {
				"cameraScreenScale": 0.25,
				"cascades": [
					"smile"
				]
			}
		}
	],
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"none": {
			"type": "image",
			"path": "image/none.png",
			"width": 1,
			"height": 1
		},
		"smile": {
			"type": "text",
			"path": "text/smile.xml",
			"global": true
		}
	},
	"globalScripts": [
		"node_modules/@dera-/akashic-webcamera-input-plugin/lib/index.js",
		"node_modules/@dera-/akashic-webcamera-input-plugin/externals/opencv.js",
		"node_modules/@dera-/akashic-webcamera-input-plugin/externals/opencvUtils.js"
	],
	"environment": {
		"sandbox-runtime": "3"
	}
}