{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/main.js",
	"operationPlugins": [
		{
			"code": 10,
			"script": "@dera-/akashic-webcamera-input-plugin",
			"option": {
				"cascades": [
					"hand"
				]
			}
		}
	],
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"hand": {
			"type": "text",
			"path": "text/hand.xml",
			"global": true
		},
		"none": {
			"type": "image",
			"path": "image/none.png",
			"width": 1,
			"height": 1
		}
	},
	"globalScripts": [
		"node_modules/@dera-/akashic-webcamera-input-plugin/lib/index.js",
		"node_modules/@dera-/akashic-webcamera-input-plugin/lib/opencv.js",
		"node_modules/@dera-/akashic-webcamera-input-plugin/lib/opencvUtils.js"
	],
	"moduleMainScripts": {
		"@dera-/akashic-webcamera-input-plugin": "node_modules/@dera-/akashic-webcamera-input-plugin/lib/index.js"
	},
	"environment": {
		"sandbox-runtime": "3"
	}
}